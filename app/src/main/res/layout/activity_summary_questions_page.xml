<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.PlaceholderFragment">

    <Button
        android:id="@+id/button_sumqNext"
        android:layout_width="161dp"
        android:layout_height="70dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="@string/NextButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button_sumqPrevious"
        android:layout_width="158dp"
        android:layout_height="69dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_endMatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:text="@string/CurrentMatch"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_endTeamNumber"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:layout_marginEnd="32dp"
        android:text="@string/CurrentTeam"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_endMatch" />

    <ImageView
        android:id="@+id/imageView_btEndIndicator"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/logo_description"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/background_light" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="316dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/BrokeDownText"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:gravity="start|center_horizontal|center_vertical"
        android:text="@string/ShootFromLaunchpad"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/LostCommText"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="225dp"
        android:layout_height="wrap_content"
        android:gravity="start|center_horizontal|center_vertical"
        android:text="@string/OppAllianceCargoText"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <Button
        android:id="@+id/ShootFromY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/ScoreOppY"
        app:layout_constraintTop_toTopOf="@+id/textView24" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/SubsystemBrokeText"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="start|center_horizontal|center_vertical"
        android:text="@string/ShootWhileText"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="start|center_horizontal|center_vertical"
        android:text="@string/DefenseAgainstText"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/PlayedDefenseText"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/BrokeDownN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/BrokeDownY"
        app:layout_constraintTop_toTopOf="@+id/BrokeDownY" />

    <Button
        android:id="@+id/LostCommN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/BrokeDownN"
        app:layout_constraintTop_toTopOf="@+id/LostCommY" />

    <Button
        android:id="@+id/ShootFromN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/ScoreOppN"
        app:layout_constraintTop_toTopOf="@+id/ShootFromY" />

    <Button
        android:id="@+id/SubsystemN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/LostCommN"
        app:layout_constraintTop_toTopOf="@+id/SubsystemY" />

    <Button
        android:id="@+id/BrokeDownY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="120dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

    <Button
        android:id="@+id/SubsystemY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/LostCommY"
        app:layout_constraintTop_toTopOf="@+id/textView19" />

    <Button
        android:id="@+id/LostCommY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/BrokeDownY"
        app:layout_constraintTop_toTopOf="@+id/textView18" />

    <Button
        android:id="@+id/ScoreOppY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/SubsystemY"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <Button
        android:id="@+id/ScoreOppN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/SubsystemN"
        app:layout_constraintTop_toTopOf="@+id/ScoreOppY" />

    <Button
        android:id="@+id/GroundPickupY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="50dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <Button
        android:id="@+id/ShootWhileY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/DefenseAgainstThemY"
        app:layout_constraintTop_toTopOf="@+id/textView13" />

    <Button
        android:id="@+id/DefenseAgainstThemN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/PlayedDefenseN"
        app:layout_constraintTop_toTopOf="@+id/DefenseAgainstThemY" />

    <Button
        android:id="@+id/DefenseAgainstThemY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/PlayedDefenseY"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <Button
        android:id="@+id/ShootWhileN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/DefenseAgainstThemN"
        app:layout_constraintTop_toTopOf="@+id/ShootWhileY" />

    <Button
        android:id="@+id/PlayedDefenseN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/TerminalPickupN"
        app:layout_constraintTop_toTopOf="@+id/PlayedDefenseY" />

    <Button
        android:id="@+id/PlayedDefenseY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/TerminalPickupY"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <Button
        android:id="@+id/TerminalPickupY"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/GroundPickupY"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <Button
        android:id="@+id/TerminalPickupN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/GroundPickupN"
        app:layout_constraintTop_toTopOf="@+id/TerminalPickupY" />

    <Button
        android:id="@+id/GroundPickupN"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/GroundPickupY"
        app:layout_constraintTop_toTopOf="@+id/GroundPickupY" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/GroundPickupText"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/TerminalPickupText"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:text="@string/SummaryQuestionTitle"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/RatingText"
        android:textAlignment="center"
        android:textSize="34sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="@string/ShootFrom"
        android:textAlignment="viewStart"
        android:textSize="34sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="@+id/textView24"
        app:layout_constraintTop_toTopOf="@+id/textView29" />

    <Spinner
        android:id="@+id/spinner_Rating"
        android:layout_width="266dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/textView29"
        app:layout_constraintTop_toTopOf="@+id/textView29" />

    <Spinner
        android:id="@+id/spinner_ShootFrom"
        android:layout_width="246dp"
        android:layout_height="42dp"
        android:layout_marginStart="30dp"
        android:visibility="invisible"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toTopOf="@+id/textView30" />

</android.support.constraint.ConstraintLayout>