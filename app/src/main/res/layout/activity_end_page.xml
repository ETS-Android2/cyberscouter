<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndPage">

    <ImageView
        android:id="@+id/imageView_btEndIndicator"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/logo_description"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/background_light" />

    <Button
        android:id="@+id/button_CASuccess"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/CASuccess"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_CAFailed" />

    <Button
        android:id="@+id/button_NABrokeDown"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:text="@string/NABrokeDown"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_InsteadOfClimb2" />

    <Button
        android:id="@+id/button_CAFailed"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/CAFailed"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NAScoredCargo" />

    <Button
        android:id="@+id/button_NAPlayedDefense"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/NAPlayedDefense"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NABrokeDown" />

    <Button
        android:id="@+id/button_NAScoredCargo"
        android:layout_width="408dp"
        android:layout_height="0dp"
        android:text="@string/NAScoredCargo"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NAPlayedDefense" />

    <Button
        android:id="@+id/climbFailedbutton"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/ClimbFailed"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_BrokeDown"
        tools:ignore="DuplicateClickableBoundsCheck,DuplicateClickableBoundsCheck" />

    <Button
        android:id="@+id/playedDefenseButton"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlayedDefense"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView_InsteadOfClimb"
        tools:ignore="DuplicateClickableBoundsCheck" />

    <Button
        android:id="@+id/brokeDownButton"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/BrokeDown"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_ScoredCargo"
        tools:ignore="DuplicateClickableBoundsCheck" />

    <Button
        android:id="@+id/scoredCargoButton"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/ScoredCargo"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_PlayedDefense"
        tools:ignore="DuplicateClickableBoundsCheck" />

    <TextView
        android:id="@+id/textView_InsteadOfClimb2"
        android:layout_width="256dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="192dp"
        android:text="@string/ClimbStatus"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_ClimbFailed"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/ClimbFailed"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_BrokeDown" />

    <Button
        android:id="@+id/button_BrokeDown"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/BrokeDown"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_ScoredCargo" />

    <Button
        android:id="@+id/button_ScoredCargo"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="@string/ScoredCargo"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_PlayedDefense" />

    <Button
        android:id="@+id/button_Previous"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_Next"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/NextButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_roleTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/ClimbPageText"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_RungPosition"
        android:layout_width="408dp"
        android:layout_height="32dp"
        android:layout_marginTop="48dp"
        android:text="@string/PositionOnTheRungs"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/button_ClimbFailed" />

    <Button
        android:id="@+id/button_positionCenter"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="6dp"
        android:text="@string/PositionCenter"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/button_positionRight"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <Button
        android:id="@+id/button_positionLeft"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:text="@string/PositionLeft"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/button_positionCenter"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <Button
        android:id="@+id/button_positionRight"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:text="@string/PositionRight"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <TextView
        android:id="@+id/textView_InsteadOfClimb"
        android:layout_width="408dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="192dp"
        android:text="@string/InsteadOfClimb"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_RobotProblem"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="280dp"
        android:layout_height="244dp"
        android:layout_marginStart="500dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_RobotProblem"
        app:srcCompat="@drawable/climbrungs_2022" />

    <Button
        android:id="@+id/button_Middle"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginStart="604dp"
        android:layout_marginTop="44dp"
        android:rotation="90"
        android:text="@string/MiddleClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button_High"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginStart="532dp"
        android:layout_marginTop="44dp"
        android:rotation="90"
        android:text="@string/HighClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button_Traversal"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginStart="464dp"
        android:layout_marginTop="44dp"
        android:rotation="90"
        android:text="@string/TraversalClimb"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <Button
        android:id="@+id/button_Low"
        android:layout_width="140dp"
        android:layout_height="70dp"
        android:layout_marginStart="672dp"
        android:layout_marginTop="44dp"
        android:rotation="90"
        android:text="@string/LowClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView_RobotProblem"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="500dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="500dp"
        android:text="@string/HighestRungClimbed"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_roleTag" />

    <TextView
        android:id="@+id/textView_endgameMatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toStartOf="@+id/textView_endgameTeam"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_endgameTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_PlayedDefense"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlayedDefense"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView_InsteadOfClimb" />

</android.support.constraint.ConstraintLayout>