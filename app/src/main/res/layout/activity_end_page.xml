<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndPage">

    <TextView
        android:id="@+id/textView_Highest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="180dp"
        android:text="@string/Lowest"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView_Highest" />

    <ImageView
        android:id="@+id/imageView_btEndIndicator"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/logo_description"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/background_light" />

    <Button
        android:id="@+id/button_CAFailed"
        android:layout_width="408dp"
        android:layout_height="56dp"
        android:layout_marginTop="6dp"
        android:text="@string/CAFailed"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NAScoredCargo" />

    <Button
        android:id="@+id/button_CASuccess"
        android:layout_width="408dp"
        android:layout_height="56dp"
        android:layout_marginTop="6dp"
        android:text="@string/CASuccess"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_CAFailed" />

    <Button
        android:id="@+id/button_NABrokeDown"
        android:layout_width="408dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:text="@string/NABrokeDown"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_InsteadOfClimb2" />

    <Button
        android:id="@+id/button_NAScoredCargo"
        android:layout_width="408dp"
        android:layout_height="56dp"
        android:layout_marginTop="6dp"
        android:text="@string/NAScoredCargo"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NAPlayedDefense" />

    <Button
        android:id="@+id/button_NAPlayedDefense"
        android:layout_width="408dp"
        android:layout_height="56dp"
        android:layout_marginTop="6dp"
        android:text="@string/NAPlayedDefense"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_NABrokeDown" />

    <TextView
        android:id="@+id/textView_InsteadOfClimb2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="196dp"
        android:text="@string/ClimbStatus"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="26sp"
        app:layout_constraintEnd_toStartOf="@+id/button_Bar4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_Previous"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_Next"
        android:layout_width="226dp"
        android:layout_height="80dp"
        android:layout_marginBottom="16dp"
        android:text="@string/NextButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_roleTag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/ClimbPageText"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_RungPosition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="472dp"
        android:layout_marginEnd="132dp"
        android:text="@string/PositionOnTheRungs"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_positionRight"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="40dp"
        android:text="@string/PositionRight"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <Button
        android:id="@+id/button_positionCenter"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="6dp"
        android:text="@string/PositionCenter"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/button_positionRight"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <Button
        android:id="@+id/button_positionLeft"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="6dp"
        android:text="@string/PositionLeft"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/button_positionCenter"
        app:layout_constraintTop_toBottomOf="@+id/textView_RungPosition" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="280dp"
        android:layout_height="244dp"
        android:layout_marginTop="182dp"
        android:layout_marginEnd="112dp"
        android:rotation="90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/climbrungs_2022" />

    <Button
        android:id="@+id/button_Bar2"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:layout_marginStart="588dp"
        android:layout_marginTop="6dp"
        android:rotation="0"
        android:text="@string/MiddleClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Bar3" />

    <Button
        android:id="@+id/button_Bar3"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:layout_marginStart="588dp"
        android:layout_marginTop="6dp"
        android:rotation="0"
        android:text="@string/HighClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Bar4" />

    <Button
        android:id="@+id/button_Bar4"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:layout_marginStart="588dp"
        android:layout_marginTop="12dp"
        android:rotation="0"
        android:text="@string/TraversalClimb"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_RobotProblem" />

    <Button
        android:id="@+id/button_Bar1"
        android:layout_width="280dp"
        android:layout_height="56dp"
        android:layout_marginStart="588dp"
        android:layout_marginTop="6dp"
        android:rotation="0"
        android:text="@string/LowClimb"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_Bar2" />

    <TextView
        android:id="@+id/textView_RobotProblem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="588dp"
        android:layout_marginTop="132dp"
        android:text="@string/HighestRungClimbed"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_endgameMatch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:text="@string/CurrentMatch"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_endgameTeam"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:text="@string/CurrentTeam"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_endgameMatch" />

    <TextView
        android:id="@+id/textView_Highest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:text="@string/Highest"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/textView_endgameTeam" />

</android.support.constraint.ConstraintLayout>